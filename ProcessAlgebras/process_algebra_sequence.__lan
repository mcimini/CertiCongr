Label L ::= (a) | (b)
Process P ::= (null) | (prefixA P) | (prefixB P) | (sequence P P).

(prefixA P) --(a)--> P. 
(prefixB P) --(b)--> P.

(sequence P1 P2) --(a)--> (sequence P1' P2) <== P1 --(a)--> P1'.
(sequence P1 P2) --(a)--> P2' <== P2 --(a)--> P2' /\ P1 -/-(a)--> /\ P1 -/-(b)-->.

(sequence P1 P2) --(b)--> (sequence P1' P2) <== P1 --(b)--> P1'.
(sequence P1 P2) --(b)--> P2' <== P2 --(b)--> P2' /\ P1 -/-(a)--> /\ P1 -/-(b)-->.
